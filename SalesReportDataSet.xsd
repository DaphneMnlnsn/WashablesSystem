                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT O.order_id, C.customer_name, ISNULL(O.unit_id,'') + CHAR(13) + CHAR(10) + ISNULL(O.unit_id2,'')
                    + CHAR(13) + CHAR(10) + ISNULL(O.unit_id3, '') AS units_used,
